      RowBox[{"f", " ", "x", " ", "z"}], "+", 
      RowBox[{"g", " ", 
       SuperscriptBox["x", "2"]}], "+", 
      RowBox[{"h", " ", 
       SuperscriptBox["y", "2"]}], "+", 
      RowBox[{"i", " ", 
       SuperscriptBox["z", "2"]}]}], ",", 
